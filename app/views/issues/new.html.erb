<h3>Create new issue</h3>
<%= form_for @issue do |f| %>
	<%= f.label :title %><br />
	<%= f.text_field :title %><br />
	<%= f.label :description %><br />
	<%= f.text_area :description %><br />
	<%= f.label :assignee %><br />
	<%= f.text_field :assignee %><br />
	<%= f.label :project_id %><br />
	<%= f.collection_select :project_id, @projects, :id, :name %>
	<%= f.submit %>
<%end%>